page{
  background: white;
}
.head{
  background: rgb(248, 242, 152);
  height: 100rpx;
}
.ground{
  background: rgb(255, 255, 255);
}
.box{
  width: calc(100% - 20px);height: 60rpx;
}
.bgf view:last-of-type{border: 0}
.star{width: 32rpx;height:32rpx;border-radius: 50%}
.user-name{
  margin-left: 10rpx;
}
.user-avatar {
  border-right: 1px solid #eee;
  width: 100rpx;
}
.user-avatar-img {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
}
.groupName{
  height:70rpx;
  padding-left: 5px;
  font-size: 16px;
  font-weight: bold;
  line-height: 70rpx;
  background-color: #EFEFF4;
  border-radius: 10px;
  margin: 0 0 5px 0;
}

.isempty{
  display: flex;
  align-items: center;
  justify-content: center;
  height: 30px;
  width: 100%;
  font-size: 15px;
}